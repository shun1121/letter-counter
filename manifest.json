{
  "name": "sample_popup_extension",
  "description": "show current time in popup",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "minicss.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"]
}